  flex: 1;
  padding: 30rpx;
  height: 200rpx;
  background: white;
}
.userInfocard .imgView image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: #ccc;
  margin-top: 25rpx;

}
.userAvatarUrl{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background: #ccc;
  margin-top: 25rpx;
}
